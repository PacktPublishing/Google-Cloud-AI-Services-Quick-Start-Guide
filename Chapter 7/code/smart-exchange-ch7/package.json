    "@google-cloud/speech": "^1.4.0",
    "@google-cloud/translate": "^1.1.0",
    "@google-cloud/video-intelligence": "^1.1.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.4",
    "@types/jasmine": "~2.8.3",
    "@types/node": "~6.0.60",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
