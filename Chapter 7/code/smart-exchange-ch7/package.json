    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "ngx-bootstrap": "^2.0.2",
    "ngx-editor": "^3.3.0-rc.12",
